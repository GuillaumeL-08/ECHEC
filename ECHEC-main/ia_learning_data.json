{
  "position_values": {},
  "move_history": [],
  "games_played": 0,
  "last_updated": null
}
def coup_iterative(self, board) -> str:
    """Utilise l'approfondissement itératif pour chercher plus loin."""
    self.board = board
    maximizing = self.board.turn == WHITE
    best_move = None
    
    # Chercher progressivement à des profondeurs croissantes
    for current_depth in range(1, self.depth + 1):
        _, move = self.minimax(current_depth, -10**9, 10**9, maximizing)
        if move:
            best_move = move
    
    return self.board.san(best_move)
